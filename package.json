  "main": "index.js",
  "scripts": {
    "start": "cd frontend/react-app/ && webpack-dev-server --open --mode development --host 0.0.0.0 --port 80",
    "heroku-postbuild": "cd frontend/react-app/ && webpack --mode development",

    "start-gat": "cd frontend/gatsby-app/ && gatsby develop --port 80",
    "heroku-postbuild-gatsby": "cd frontend/gatsby-app/ && gatsby build",

    "start-web": "cd frontend/react-app/ && webpack-dev-server --open --mode development --host 0.0.0.0 --port 80",
    "heroku-postbuild-webpack": "webpack --mode development",

    "webpack-start": "webpack-dev-server --open --mode development",
    "webpack-build": "webpack --mode development",
    "webpack-watch": "webpack -d --watch",

    "gatsby-build": "gatsby build",
    "gatsby-develop": "gatsby develop",
    "gatsby-format": "prettier --write src/**/*.{js,jsx}",
    "gatsby-start": "npm run develop",
    "gatsby-serve": "gatsby serve",

    "json-server": "json-server --watch backend/users.json"
